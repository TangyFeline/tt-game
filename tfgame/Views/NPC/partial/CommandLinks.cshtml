@model tfgame.ViewModels.MindControlViewModel

@{
    if (Model.MindControl.Type == tfgame.Statics.MindControlStatics.MindControl__Movement)
    {
        @Html.ActionLink("Command Movement","MoveVictim","NPC", new {id=Model.Victim.Player.Id}, null)
    }
    if (Model.MindControl.Type == tfgame.Statics.MindControlStatics.MindControl__Strip)
    {
        @Html.ActionLink("Command Strip","StripVictim","NPC", new {id=Model.Victim.Player.Id}, null)
    }
}