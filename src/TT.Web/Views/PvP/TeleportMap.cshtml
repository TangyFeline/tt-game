@model IEnumerable<TT.Domain.ViewModels.Location>
           @using TT.Domain.ViewModels
@{
    ViewBag.Title = "Teleport";
}

@section menu
{
    <li><a href="@Url.Action(MVC.PvP.Play())">Back to game</a></li>
}

<p>Choose which destination you would like to teleport to.  You will be transported there instantly without any action points being used.</p>

@{
    
    foreach (Location loc in Model) {
        <a href="@Url.Action(MVC.PvP.Teleport(loc.dbName))">@loc.Name</a><br />
    }
    
}

